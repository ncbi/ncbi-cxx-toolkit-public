         [Yy]* )  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Proceeding at your own risk..." >&5
$as_echo "$as_me: WARNING: Proceeding at your own risk..." >&2;} ;;
         *       )  as_fn_error $? "Configuration has been canceled by user." "$LINENO" 5 ;;
       esac
       ;;
 esac
      echo
      ;;
   esac
fi

