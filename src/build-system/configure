            $net_sybase_root/* | $local_sybase_root/* ) ;;
            * ) { echo "$as_me:$LINENO: WARNING: ignoring non-standard SYBASE setting: $SYBASE" >&5
echo "$as_me: WARNING: ignoring non-standard SYBASE setting: $SYBASE" >&2;} ;;
