      | --with-check-tools=* | --with-ftds=[0-9]* | --with-fastcgi=[0-9]* \
      | --with-bdb=[1-9]* | --with-boost-tag=* | --with-3psw=* )
      ;;

      * )
      as_fn_error $? "$x_arg:  unknown option;  use --help to show usage"
      ;;
   esac
