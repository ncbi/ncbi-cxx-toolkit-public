         case $x in
            -L* ) dirs="$dirs `echo _$x | cut -c4-`" ;;
            -l* )
                want=lib`echo _$x | cut -c4-`-static.a
                for d in $dirs; do
                   if test -f $d/$want; then
                      x=$x-static
                      break
                   fi
                done
                ;;
         esac
         static_libs=$static_libs$sep$x
         sep=' '
      done
      GNUTLS_LIBS="$static_libs $ICONV_LIBS"
   fi
fi

